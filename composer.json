{
    "name": "armezit/kyc-jibit",
    "type": "library",
    "description": "PHP client for the Jibit Identicator Project (KYC) API",
    "keywords": [
        "kyc",
        "jibit",
        "rest-api"
    ],
    "homepage": "https://github.com/armezit/kyc-jibit",
    "license": "MIT",
    "authors": [
        {
            "name": "Armin Rezayati",
            "email": "armin.rezayati@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Armezit\\Kyc\\Jibit\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Armezit\\Kyc\\Jibit\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8",
        "ext-json": "*",
        "php-http/client-implementation": "^1",
        "php-http/discovery": "^1.14",
        "psr/http-client": "^1.0",
        "symfony/cache": "^6",
        "symfony/http-foundation": "^6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "php-http/guzzle7-adapter": "^1",
        "php-http/mock-client": "^1",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "lint": "phpcs",
        "test": "phpunit"
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
