{
    "name": "armezit/kyc-jibit",
    "type": "library",
    "description": "PHP client for the Jibit Identicator Project (KYC) API",
    "keywords": [
        "kyc",
        "jibit",
        "rest-api"
    ],
    "homepage": "https://github.com/armezit/kyc-jibit",
    "license": "MIT",
    "authors": [
        {
            "name": "Armin Rezayati",
            "email": "armin.rezayati@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Armezit\\Kyc\\Jibit\\" : "src/" }
    },
    "require": {
        "php": "^8",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "symfony/http-foundation": "^6.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.8"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "scripts": {
        "lint": "phpcs",
        "test": "phpunit"
    }
}
